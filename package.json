{
  "name": "tugas3-cardname",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tailwind:watch": "npx tailwindcss -i ./src/css/input.css -o ./src/css/global.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/css/input.css -o ./src/css/global.css",
